generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    Id         String       @id @default(uuid())
    Name       String?
    Email      String       @unique
    Recordings Recordings[]
}

model Recordings {
    Id           Int      @id @default(autoincrement())
    UserId       String
    RemoteUserId String
    Recording    String
    Date         DateTime
    Processed    Boolean
    PublicUrl    String?
    User         User     @relation(fields: [UserId], references: [Id])
}
